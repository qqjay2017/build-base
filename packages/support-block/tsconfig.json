{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "strict": false,
    
    "noImplicitAny": false,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "declaration": true,
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["./src/**/*"]
}
